{
  "manifest_version": 2,

  "name": "PSafe",
  "description": "Generates and stores passwords in a secure matter",
  "version": "0.1",
  "options_page": "options.html",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Settings"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}